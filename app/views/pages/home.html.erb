<div class="container text-center">
  <div class="content">
    <div class="content-inner">
      <h1 class="team-title">TEAM8</h1>
      <div class="divider"></div>
      <ul class="features-list">
        <li>CONNECT</li>
        <li>TRAIN</li>
        <li>THRIVE</li>
      </ul>
    </div>
  </div>
  <div class="buttons-landing">
    <button class="btn btn-lg login-btn">
      <%= link_to user_session_path, class: "undecorated-link"  do %>
        <span>Login</span>
      <% end %>
    </button>
    <button class="btn btn-lg signup-btn">
      <%= link_to new_user_registration_path, class: "undecorated-link" do %>
        <span>Sign-up</span>
      <% end %>
    </button>
  </div>
</div>
