<div class="container">
  <div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4), rgba(0,0,0,0.4)), url(<%= asset_path('') %>);">
    <div class="container">
      <h1><strong>TEAM8</strong></h1>
      <p>CONNECT, TRAIN, THRIVE</p>
    </div>
  </div>
  <h1>Browse profiles with similar interests to you</h1>
 <% @users.each do |user| %>
  <div class="user-card" style="width: 18rem; margin-bottom: 20px;">
    <div class="card-body">
      <h5 class="card-title"><%= user.first_name %> <%= user.last_name %></h5>
      <p class="card-text">Age: <%= user.age %></p>
    </div>
  </div>
<% end %>
</div>
